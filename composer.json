{
    "name": "octava/symfony-json-schema-form",
    "description": "Clone of limenius/liform. Symfony json schema form is a library for serializing Symfony Forms into JSON schema.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Octava\\SymfonyJsonSchemaForm\\": "src/",
            "Octava\\SymfonyJsonSchemaBundle\\": "bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Octava\\SymfonyJsonSchemaForm\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "The Rat",
            "email": "theratg@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">8.1",
        "symfony/form": "^6.4",
        "symfony/translation": "^6.4",
        "symfony/serializer": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/translation-contracts": "^3.4"
    },
    "require-dev": {
        "phpro/grumphp": "^2.11",
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.72"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
